<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="viewport" content="width=s, initial-scale=1.0">
    <title><%- data.base.title %></title>
    <meta name="description" content="<%- data.base.description %>">
    <link rel="stylesheet" href="css/style.css">
    <link rel="icon" type="image/png" href="images/common/favicon.png">
    <link rel="apple-touch-icon" href="images/common/apple-touch-icon.png">
    <!-- OGP -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="">
    <meta property="og:title" content="<%- data.base.description %>">
    <meta property="og:image" content="images/common/ogp.png">
    <meta property="og:description" content="Description Here">
    <meta property="og:site_name" content="<%- data.base.title %>">
    <meta property="og:locale" content="ja_JP">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="630">
    <!-- Twitter Card -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="<%- data.base.title %>">
    <meta name="twitter:creator" content="">
    <meta name="twitter:url" content="">
    <meta name="twitter:title" content="<%- data.base.title %>">
    <meta name="twitter:description" content="<%- data.base.description %>">
    <meta name="twitter:image" content="images/common/ogp.png">
</head>